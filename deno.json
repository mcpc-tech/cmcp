{
  "name": "@mcpc/cmcp",
  "version": "0.0.13-beta.2",
  "description": "Client-Tool-Execution MCP Server",
  "author": "mcpc.tech",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcpc-tech/cmcp.git"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "dynamic",
    "client-execution",
    "proxy",
    "tools",
    "ai",
    "llm"
  ],
  "exports": {
    ".": "./mod.ts",
    "./server": "./decorators/client_exec_server.ts",
    "./client": "./decorators/client_exec_client.ts",
    "./types": "./shared/types.ts"
  },
  "tasks": {
    "build": "deno check --all .",
    "test": "deno test --allow-all",
    "test:quiet": "deno test --quiet --allow-all",
    "fmt": "deno fmt",
    "lint": "deno lint"
  },
  "imports": {
    "@hono/zod-openapi": "npm:@hono/zod-openapi@^0.19.9",
    "@modelcontextprotocol/sdk": "npm:@modelcontextprotocol/sdk@^1.15.0",
    "@std/assert": "jsr:@std/assert@^1.0.13",
    "@std/http": "jsr:@std/http@^1.0.19",
    "zod": "npm:zod@^3.25.74"
  },
  "lint": {
    "exclude": ["examples/"]
  },
  "publish": {
    "exclude": [
      "*.lock",
      ".vscode/",
      ".git/",
      ".github/",
      "examples/",
      "*_test.ts",
      "*_integration_test.ts"
    ]
  }
}
